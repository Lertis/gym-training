{
	"name": "gym-training",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "\"node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng serve --host 0.0.0.0 --port 7000 -o\"",
		"build: prod": "node --max_old_space_size=6000 ./node_modules/@angular/cli/bin/ng build --prod",
		"test-j": "jest --maxWorkers=5 --config ./jest.config.js",
		"test-j:o": "jest --maxWorkers=5 --onlyChanged --config ./jest.config.js",
		"test-j-coverage": "jest --coverage --maxWorkers=5 --config ./jest.config.js",
		"test-j-coverage:o": "jest --coverage --maxWorkers=5 --onlyChanged --config ./jest.config.js",
		"test-j-snapshots": "npm run test-j -- -u",
		"test-j-cache": "jest --clearCache",
		"lint": "ng lint"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "11.2.5",
		"@angular/cdk": "11.2.4",
		"@angular/common": "11.2.5",
		"@angular/compiler": "11.2.5",
		"@angular/core": "11.2.5",
		"@angular/forms": "11.2.5",
		"@angular/material": "11.2.4",
		"@angular/platform-browser": "11.2.5",
		"@angular/platform-browser-dynamic": "11.2.5",
		"@angular/router": "11.2.5",
		"@ngrx/effects": "11.0.1",
		"@ngrx/store": "11.0.1",
		"@ngrx/store-devtools": "11.0.1",
		"rxjs": "6.6.0",
		"tslib": "2.0.0",
		"zone.js": "0.11.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "0.1102.3",
		"@angular/cli": "11.2.4",
		"@angular/compiler-cli": "11.2.5",
		"@types/jest": "26.0.20",
		"@types/lodash": "4.14.168",
		"@types/node": "12.11.1",
		"codelyzer": "6.0.0",
		"jest": "26.6.3",
		"jest-preset-angular": "8.4.0",
		"lodash": "4.17.21",
		"moment": "^2.29.1",
		"protractor": "7.0.0",
		"ts-node": "8.3.0",
		"tslint": "6.1.0",
		"typescript": "4.1.5"
	}
}
